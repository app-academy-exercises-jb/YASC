<script type="text/javascript">
    window.csrf = "<%= form_authenticity_token %>";

    <% if session[:current_user] %>
    window.currentUser = {
      id: "<%= session[:current_user]["id"] %>",
      email: "<%= session[:current_user]["email"] %>",
      session_token: "<%= session[:current_user]["session_token"] %>"
    };
    <% end %>
</script>

<%= react_component("app", {}, {}) %>